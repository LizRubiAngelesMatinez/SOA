	[Descripcion] [varchar](200) NULL,
	[Nivel] [varchar](50) NULL,
	[Precio] [int] NULL,
	[Valoracion] [float] NULL,
	[Duracion] [int] NULL,
	[FecLanzamiento] [date] NULL,
	[Estado] [varchar](100) NULL,
